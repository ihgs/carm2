

<h1>Listing Admin Daily Reports</h1>


<table class="table">
  <thead>
    <tr>
      <th>Date</th>
      <th>Grade</th>
      <th>Subject</th>
      <th>Note</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <tr ng-repeat="daily_report in daily_reports" >
      <td>{{daily_report.date}}</td>
      <td>{{daily_report.grade}}</td>
      <td>{{daily_report.subject}}</td>
      <td>{{daily_report.note}}</td>
      <td><a ui-sref="daily_reports.show({id: daily_report.id})">Show</a></td>
      <td><a ui-sref="daily_reports.edit({id: daily_report.id})">Edit</a></td>
    </tr>
  </tbody>
</table>

<br>
<a ui-sref="daily_reports.new">新規作成</a>
