'use strict';

function showTestResultDirective(){
  return {
    restrict: 'A',
    scope: {
      schoolTestResult: '='
    },
    bindToController:{
      result: '=',
      subjects: '=',
      schoolTestId: '='
    },
    controller: ShowTestResultController,
    controllerAs: 'ctrl',
    templateUrl: '<%= asset_path 'app/school_tests/show_test_result.html' %>'
  }
}
