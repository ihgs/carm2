<p id="notice"><%= notice %></p>

<h1>Listing Admin Daily Reports</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Grade</th>
      <th>Subject</th>
      <th>Note</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @daily_reports.each do |daily_report| %>
      <tr>
        <td><%= daily_report.date %></td>
        <td><%= daily_report.grade %></td>
        <td><%= daily_report.subject %></td>
        <td><%= daily_report.note %></td>
        <td><%= link_to 'Show', [:admin, daily_report] %></td>
        <td><%= link_to 'Edit', [:edit, :admin, daily_report] %></td>
        <td><%= link_to 'Destroy', [:admin, daily_report], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Daily report', new_admin_daily_report_path %>
